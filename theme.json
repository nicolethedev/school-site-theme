{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"color": "#023250",
					"name": "Cobalt Blue",
					"slug": "cobalt-blue"
				},
				{
					"color": "#B3E3E1",
					"name": "Mint Green",
					"slug": "mint-green"
				},
				{
					"color": "#FFC67D",
					"name": "Bright Coral",
					"slug": "bright-coral"
				},
				{
					"color": "#F8F1E8",
					"name": "Sandy Sand",
					"slug": "sandy-sand"
				},
				{
					"color": "#A9362F",
					"name": "Red Red Wine",
					"slug": "red-red-wine"
				},
				{
					"color": "#778F91",
					"name": "Icy Blue",
					"slug": "icy-blue"
				},
				{
					"color": "#903D22",
					"name": "Burnt Orange",
					"slug": "burnt-orange"
				},
				{
					"color": "#9A8069",
					"name": "Taupe",
					"slug": "taupe"
				}
			]
		},
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [ "%", "px", "em", "rem", "vh", "vw" ]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"name": "Lato",
					"slug": "lato",
					"fontFamily": "'Lato', sans-serif",
					"fontFace": [
						{
							"fontFamily": "Lato",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": ["file:./assets/fonts/Lato-Regular.ttf"]
						},
						{
							"fontFamily": "Lato",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": ["file:./assets/fonts/Lato-Bold.ttf"]
						}
					]
				},
				{
					"name": "Open Sans",
					"slug": "open-sans",
					"fontFamily": "'Open Sans', sans-serif",
					"fontFace": [
						{
							"fontFamily": "Open Sans",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": ["file:./assets/fonts/OpenSans-Regular.ttf"]
						},
						{
							"fontFamily": "Open Sans",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": ["file:./assets/fonts/OpenSans-Bold.ttf"]
						}
					]
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "13px",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "20px",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "36px",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "42px",
					"name": "Extra Large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
    "typography": {
        "fontFamily": "var(--wp--preset--font-family--open-sans)",
        "fontSize": "var(--wp--preset--font-size--medium)",
        "lineHeight": "1.6"
    },
    "elements": {
        "link": {
            "color": {
                "text": "var(--wp--preset--color--cobalt-blue)"
            },
            "typography": {
                "fontFamily": "var(--wp--preset--font-family--open-sans)"
            }
        },
        "h1": {
            "typography": {
                "fontFamily": "var(--wp--preset--font-family--lato)",
                "fontSize": "var(--wp--preset--font-size--x-large)",
                "fontWeight": "700"
            }
        },
        "h2": {
            "typography": {
                "fontFamily": "var(--wp--preset--font-family--lato)",
                "fontSize": "var(--wp--preset--font-size--large)",
                "fontWeight": "600"
            }
        },
        "h3": {
            "typography": {
                "fontFamily": "var(--wp--preset--font-family--lato)",
                "fontSize": "calc(var(--wp--preset--font-size--large) * 0.8)",
                "fontWeight": "600"
            }
        },
        "h4": {
            "typography": {
                "fontFamily": "var(--wp--preset--font-family--lato)"
            }
        },
        "h5": {
            "typography": {
                "fontFamily": "var(--wp--preset--font-family--lato)"
            }
        },
        "h6": {
            "typography": {
                "fontFamily": "var(--wp--preset--font-family--lato)"
            }
        }
    },
    "blocks": {
        "core/button": {
            "color": {
                "background": "var(--wp--preset--color--red-red-wine)",
                "text": "var(--wp--preset--color--sandy-sand)"
            },
            "typography": {
                "fontFamily": "var(--wp--preset--font-family--open-sans)",
                "textTransform": "none"
            }
        },
        "core/post-title": {
            "typography": {
                "fontFamily": "var(--wp--preset--font-family--lato)",
                "fontSize": "var(--wp--preset--font-size--x-large)",
                "fontWeight": "700"
            }
        },
        "core/caption": {
            "typography": {
                "fontFamily": "var(--wp--preset--font-family--open-sans)",
                "fontSize": "var(--wp--preset--font-size--small)",
                "fontStyle": "italic"
            }
        }
    }
},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3
}
